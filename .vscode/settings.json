{
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.files.associations": {
    "*.module.ts": "nest-module",
    "*.controller.ts": "nest-controller",
    "*.service.ts": "nest-service"
  }
}
